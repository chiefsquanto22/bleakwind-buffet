<UserControl x:Class="PointOfSale.SmokehouseSkeletonCustom"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PointOfSale"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <StackPanel>
            <CheckBox Content="Sausage Link"    IsChecked="{Binding Path=SausageLink}"/>
            <CheckBox Content="Egg"             IsChecked="{Binding Path=Egg}"/>
            <CheckBox Content="Hash Browns"     IsChecked="{Binding Path=HashBrowns}"/>
            <CheckBox Content="Pancake"         IsChecked="{Binding Path=Pancake}"/>
        </StackPanel>
    </Grid>
</UserControl>

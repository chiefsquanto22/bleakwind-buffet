<UserControl x:Class="PointOfSale.CustomControls.SmokehouseSkeletonCustom"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PointOfSale.CustomControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <StackPanel>
            <CheckBox Content="Sausage Link"/>
            <CheckBox Content="Egg"/>
            <CheckBox Content="Hash Browns"/>
            <CheckBox Content="Pancake"/>
        </StackPanel>
        <Button Content="Done" VerticalAlignment="Bottom"/>
    </Grid>
</UserControl>

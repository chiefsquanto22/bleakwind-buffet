<UserControl x:Class="PointOfSale.GardenOrcOmeletteCustom"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PointOfSale"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <StackPanel>
            <CheckBox Content="Broccoli" IsChecked="True"/>
            <CheckBox Content="Mushrooms" IsChecked="True"/>
            <CheckBox Content="Tomato" IsChecked="True"/>
            <CheckBox Content="Cheddar" IsChecked="True"/>
        </StackPanel>
    </Grid>
</UserControl>
